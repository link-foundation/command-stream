{
  "name": "command-stream",
  "version": "0.0.1",
  "description": "Modern $ shell utility library with streaming, async iteration, and EventEmitter support, optimized for Bun runtime",
  "type": "module",
  "main": "$.mjs",
  "exports": {
    ".": "./$.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/link-foundation/command-stream.git"
  },
  "homepage": "https://github.com/link-foundation/command-stream#readme",
  "bugs": {
    "url": "https://github.com/link-foundation/command-stream/issues"
  },
  "scripts": {
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:features": "bun test *.features.test.mjs",
    "test:comparison": "bun test *.features.test.mjs --reporter=verbose",
    "test:readme": "bun test readme-examples.test.mjs",
    "test:sync": "bun test sync.test.mjs",
    "test:builtin": "bun test builtin-commands.test.mjs"
  },
  "keywords": [
    "shell",
    "command",
    "streaming",
    "async",
    "iteration",
    "eventemitter",
    "bun",
    "node",
    "cross-runtime"
  ],
  "author": "link-foundation",
  "license": "Unlicense",
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=20.0.0"
  },
  "files": [
    "$.mjs",
    "README.md",
    "LICENSE"
  ]
}